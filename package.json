        "sqlite3": "^5.1.7"
    },
    "engines": {
        "node": ">=18"
    },
    "devDependencies": {
        "@svgr/webpack": "^8.1.0",
        "eslint": "8.57.0",
        "eslint-config-next": "14.2.5",
        "prettier": "3.3.3",
        "sequelize-cli": "^6.6.2"
    },
    "imports": {
        "#root/*.js": "./*.js",
        "#root/*.css": "./*.css",
        "#root/*.svg": "./*.svg"
